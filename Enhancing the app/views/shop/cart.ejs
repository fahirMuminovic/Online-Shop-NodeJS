<%- include('../includes/head.ejs') %>
</head>

<body>
  <%- include('../includes/navigation.ejs') %>
  <main>
    <% if(cartItems.length > 0) {%>
    <ul>
      <% cartItems.forEach( item => { %>
      <li><%= item.productData.title%> (<%= item.quantity%>)</li>
      <% }) %>
    </ul>
    <% } else { %>
    <h1>No products in cart</h1>
    <% } %>
  </main>
  <%- include('../includes/end.ejs') %>